# Domínio & TLS
N8N_HOST=n8n.SEUDOMINIO.com.br
LETSENCRYPT_EMAIL=seu-email@seudominio.com.br

# n8n
N8N_IMAGE_TAG=1.99.1         # fixe uma versão estável
GENERIC_TIMEZONE=America/Sao_Paulo
TZ=America/Sao_Paulo
WORKER_CONCURRENCY=5

# Segurança
# Gere uma chave robusta (>= 32 chars); a MESMA chave deve estar no main e em todos os workers.
N8N_ENCRYPTION_KEY=troque_por_uma_chave_muito_forte
N8N_PUBLIC_API_DISABLED=true
N8N_PUBLIC_API_SWAGGERUI_DISABLED=true

# Banco (Postgres)
POSTGRES_DB=n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD=troque_essa_senha

# Redis
REDIS_PASSWORD=troque_essa_senha
